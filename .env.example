# FreshRSS connection
FRESHRSS_BASE_URL=https://your-freshrss.example
FRESHRSS_USERNAME=your_username
FRESHRSS_API_PASSWORD=your_api_password
FRESHRSS_LABEL=

# Digest behaviour
TIMEZONE=Europe/London
LOOKBACK_HOURS=24
MAX_ITEMS=1000
BLOCKLIST_PHRASES=

# Email settings
FROM_EMAIL=pr-digest@example.com
TO_EMAILS=recipient@example.com
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USERNAME=smtp_user
SMTP_PASSWORD=smtp_password
SMTP_USE_TLS=true
